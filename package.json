{
  "name": "orm-com-nodejs-api-sequelize-e-mysql",
  "version": "1.0.0",
  "description": "Este projeto tem como finalidade aprofundar o conhecimento sobre gerenciamento de conteudos usando o padrão MVC através do Sequelize aplicado a uma API REST em NodeJS",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon api/index.js",
    "model:create": "npx sequelize-cli model:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed:generator": "npx sequelize-cli seed:generate",
    "seed:populate": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo"
  },
  "repository": "https://github.com/Isaachintosh/orm-com-nodejs-api-sequelize-e-mysql.git",
  "author": "Isaac Mello aka @isaachintosh",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mysql2": "^2.3.0",
    "path": "^0.12.7",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
